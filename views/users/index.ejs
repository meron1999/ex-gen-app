<%- include('../partials/header.ejs') %>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Mail</th>
                <th>Age</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            <% for(var i in content) { %>
                <tr>
                    <td><%= content[i].id %></td>
                    <td><%= content[i].name %></td>
                    <td><%= content[i].mail %></td>
                    <td><%= content[i].age %></td>
                    <td>
                        <a href="/users/edit?id=<%= content[i].id %>" class="btn btn-sm btn-info">Edit</a>
                        <a href="/users/delete?id=<%= content[i].id %>" class="btn btn-sm btn-danger">Delete</a>
                    </td>
                </tr>
            <% } %>
        </tbody>
    </table>
    <p><a href="/users/add" class="btn btn-primary">Add New User</a></p>
<%- include('../partials/footer.ejs') %>